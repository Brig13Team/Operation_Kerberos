{
    "Dorbedo Header": {
        "prefix": "header",
        "body": [
            "#include \"script_component.hpp\"",
            "/**",
            " * Name: dorb_${1:COMPONENT}_$TM_FILENAME_BASE",
            " *",
            " * Author: Dorbedo",
            " * ${2:description}",
            " *",
            " * Arguments:",
            " * 0: <${3:TYPENAME}> ${4:argument name}",
            " *",
            " * Return Value:",
            " * <${5:TYPENAME}> ${6:return name}",
            " *",
            " */",
            "",
            "$0"
        ],
        "description": "Creates Dorbedos Function Header"
    },
    "CHECK": {
        "prefix": "CHECK",
       "body": [
           "If ($1) exitWith {$2};",
          "$0"
       ],
       "description": "Exit if condition doesn't match"
    }
}
